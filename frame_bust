<?xml version="1.0" encoding="utf-8" ?>  
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">  
<j:set var="jvar_url" value="${sysparm_url}" />
  
<script type="text/javascript">  
  if("${gs.getProperty('glide.authenticate.cas.loginurl')}"){
    top.location.replace("${gs.getProperty('glide.authenticate.cas.loginurl')}" + "?service=" + "${jvar_url}");
  }  
</script>  
</j:jelly>  
